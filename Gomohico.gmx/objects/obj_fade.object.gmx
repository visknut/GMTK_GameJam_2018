{
    draw_set_alpha(ease_in_quint(alarm[3], 0, 1, FADE_TIME/12));
} else {
    draw_set_alpha(0);
}

// Draw text.
draw_set_colour(colour_text);
draw_set_font(fnt_xxl);
draw_set_valign(fa_middle);
draw_set_halign(fa_center);
draw_text(360, 280, text);

// Reset aligned.
